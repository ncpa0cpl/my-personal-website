{
  "devDependencies": {
    "@ncpa0cpl/nodepack": "^2.0.1",
    "@types/adm-zip": "^0.5.0",
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.12",
    "@types/csso": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.5",
    "@types/glob": "^7.2.0",
    "@types/js-beautify": "^1.13.3",
    "@types/lodash.trim": "^4.5.7",
    "@types/luxon": "^3.0.1",
    "@types/node": "^17.0.31",
    "@types/session-file-store": "^1.2.2",
    "@types/uuid": "^8.3.4",
    "nodemon": "^2.0.20",
    "supervisor": "^0.12.0"
  },
  "name": "node",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "scripts": {
    "start:dev": "NODE_ENV=development nodemon --ignore ./dist/server.log --ignore ./dist/sessions/ --watch ./dist -e js,mjs,cjs,json,css,svg ./dist/legacy/start-server.js",
    "start": "NODE_ENV=production node ./dist/legacy/start-server.js",
    "build": "rm -rf ./dist/legacy && node ./scripts/build.cjs",
    "watch": "node ./scripts/watch.cjs",
    "tsc": "tsc",
    "test:lint": "eslint .",
    "test:tsc": "tsc --noEmit",
    "test:jest": "jest --coverage",
    "test": "yarn test:tsc && yarn test:lint && yarn test:jest",
    "orm": "typeorm-ts-node-commonjs -d ./dist/legacy/database/configuration/data-source.js",
    "generate:migrations": "yarn build && ./scripts/generate-migrations.sh",
    "run-migrations": "yarn orm migration:run",
    "clear:dist": "find ./dist -iregex '.*\\.\\(js\\|ts\\)' -exec rm {} \\; || echo",
    "generate-svg-names": "node ./scripts/generate-svg-names.cjs"
  },
  "dependencies": {
    "adm-zip": "^0.5.9",
    "async-await-queue": "^1.2.1",
    "axios": "^0.27.2",
    "clsx": "^1.2.1",
    "cors": "^2.8.5",
    "csso": "^5.0.5",
    "dilswer": "^1.2.0",
    "error-result": "ncpa0cpl/error-result#1.0.0",
    "esbuild": "^0.15.7",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "glob": "^8.0.1",
    "js-beautify": "^1.14.4",
    "jsxte": "^2.1.0",
    "lodash.trim": "^4.5.1",
    "luxon": "^3.0.3",
    "mutex-decorators": "ncpa0cpl/repository-mutex#1.0.2",
    "node-html-parser": "^5.4.2",
    "plain-tag": "^0.1.3",
    "puppeteer": "^17.1.0",
    "reflect-metadata": "^0.1.13",
    "session-file-store": "^1.5.0",
    "sqlite3": "^5.0.8",
    "status-code-enum": "^1.0.0",
    "strip-css-comments": "^5.0.0",
    "typeorm": "^0.3.9",
    "uuid": "^8.3.2",
    "uuid-by-string": "^3.0.7",
    "winston": "^3.7.2"
  },
  "nodemonConfig": {
    "delay": 1000
  }
}
